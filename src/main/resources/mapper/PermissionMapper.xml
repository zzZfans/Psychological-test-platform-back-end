<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqjtu.mindassess.mapper.PermissionMapper">

    <resultMap id="permissionInfoVo" type="com.cqjtu.mindassess.pojo.vo.PermissionInfoVo">
        <result property="id" column="id"/>
        <result property="icon" column="icon"/>
        <result property="permissionName" column="permission_name"/>
        <result property="parentId" column="parent_id"/>
    </resultMap>
    <select id="listPermissionInfoVo" resultMap="permissionInfoVo">
        select id,icon,permission_name,parent_id
        from permission
        where is_deleted = 0
        order by sort ASC
    </select>
</mapper>
